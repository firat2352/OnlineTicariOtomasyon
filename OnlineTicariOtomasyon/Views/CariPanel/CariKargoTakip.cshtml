@using OnlineTicariOtomasyon.Models.Siniflar

@model List<KargoTakip>

@{
    ViewBag.Title = "CariKargoTakip";
    Layout = "~/Views/Shared/_CariLayout.cshtml";
}



<br />
<table class="table table-striped" style="margin-top:20px">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>

    </tr>


    @foreach (KargoTakip kargoTakip in Model)
    {
        <tr>
            <td>@kargoTakip.Aciklama</td>
            <td>@kargoTakip.TarihZaman</td>

        </tr>
    }
</table>



